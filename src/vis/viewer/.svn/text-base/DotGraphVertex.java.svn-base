package vis.viewer;

import edu.uci.ics.jung.graph.impl.SparseVertex;

public class DotGraphVertex extends SparseVertex {

	protected String name;

	public DotGraphVertex(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public String toString() {
		return getName();
	}
}